tall <- "https://www.motor.no/aktuelt/motors-store-vintertest-av-rekkevidde-pa-elbiler/217132"  %>%
read_html() %>%
html_nodes(xpath = '/html/body/article/section/div[4]/div[4]/div[1]/table') %>% # henter inn tabellen ved 친 kopiere xpathen fra sidekildenen
html_table(header = 1, fill = TRUE) %>% # setter at f칮rste rad blir header for hver kolonne
as.data.frame() %>%
filter(!str_detect(Avvik, "x")) %>% # filterer bort de modellene uten data
separate(`WLTP.tall`, sep = "/", into=c("WLTP","kWh")) %>% # Separerer WLTP-tall til to kolonner for 친 forenkle og at det ser ryddigere ut
mutate(STOPP = as.numeric(gsub("km", "", STOPP))) %>% #fjerner km for 친 kunne gj칮re det numeric
mutate(WLTP = as.numeric(gsub("km", "", WLTP)))  %>% #fjerner km for 친 kunne gj칮re det numeric
rename("Modell" = Modell..temp..varierte.fra.0..til..10..) %>% # endrer navn p친 titel for 친 f친 ggplotly til 친 bli penere
ggplot(., aes(x = WLTP, y = STOPP, label = Modell)) +
